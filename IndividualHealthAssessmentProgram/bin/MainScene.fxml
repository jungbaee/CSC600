            <Label fx:id="BMILabel" text="BMI" GridPane.rowIndex="2" />
            <Label fx:id="HDLLabel" text="Cholestrerol Levels HDL" GridPane.rowIndex="3" />
            <Label fx:id="LDLLabel" text="Cholestrerol Levels LDL" GridPane.rowIndex="4" />
            <Label fx:id="hemoglobinLabel" text="Hemoglobin A/1c" GridPane.rowIndex="5" />
